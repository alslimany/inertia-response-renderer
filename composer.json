{
    "name": "laravel-arcanist/inertia-response-renderer",
    "type": "library",
    "description": "Inertia response renderer for Arcanist",
    "license": "MIT",
    "authors": [
        {
            "name": "Kai Sassnowski",
            "email": "me@kai-sassnowski.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "inertiajs/inertia-laravel": "^0.5.2",
        "laravel-arcanist/arcanist": "^0.7.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.13",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.17 || ^7.6",
        "phpunit/phpunit": "^9.0",
        "spatie/phpunit-watcher": "^1.23"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arcanist\\InertiaResponseRendererServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Arcanist\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanist\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
